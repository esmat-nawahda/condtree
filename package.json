{
  "name": "condtree",
  "version": "1.0.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jest": {
    "testPathIgnorePatterns": [
      "lib/**/*.d.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
  },
  "scripts": {
    "test": "jest --config jest.config.json",
